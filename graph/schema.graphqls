# GraphQL schema example
#
# https://gqlgen.com/getting-started/

# NOTE: `scalar Time`の意味：http://spec.graphql.org/June2018/#example-cb7e7
scalar Time

type Query {
  GetUsers: [User!]!
}

type Mutation {
  # NOTE: 今後実装する
  createUser(input: NewUser!): User!
}

type User {
  Uid: String!
  Email: String!
  LastName: String!
  FirstName: String!
  Gender: String!
  CreatedAt: Time!
  UpdatedAt: Time!
  DeletedAt: Time!
}

input NewUser {
  name: String!
}
